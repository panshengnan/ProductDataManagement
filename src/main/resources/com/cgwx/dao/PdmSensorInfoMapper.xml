<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cgwx.dao.PdmSensorInfoMapper" >
  <resultMap id="BaseResultMap" type="com.cgwx.data.entity.PdmSensorInfo" >
    <result column="sensor_code" property="sensorCode" jdbcType="INTEGER" />
    <result column="sensor_description" property="sensorDescription" jdbcType="VARCHAR" />
  </resultMap>
  <insert id="insert" parameterType="com.cgwx.data.entity.PdmSensorInfo" >
    insert into pdm_sensor_info (sensor_code, sensor_description)
    values (#{sensorCode,jdbcType=INTEGER}, #{sensorDescription,jdbcType=VARCHAR})
  </insert>
  <select id="selectAll" resultMap="BaseResultMap" >
    select sensor_code, sensor_description
    from pdm_sensor_info
  </select>
</mapper>